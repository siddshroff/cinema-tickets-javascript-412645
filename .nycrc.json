{
  "report-dir": "./.coverage",
  "check-coverage": true,
  "lines": 80,
  "statements": 80,
  "functions": 80,
  "branches": 80,
  "reporter": [
    "cobertura",
    "text-summary",
    "lcov",
    "text",
    "html"
  ],
  "include": [
    "src/**/*.js"
  ],
  "exclude":[
    "**/*.spec.js",
    "src/thirdparty/**/*.js"
  ]
}